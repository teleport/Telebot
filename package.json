{
  "name": "bubert",
  "version": "0.2.0",
  "private": true,
  "author": "Mihkel Sokk <mihkel@teleport.org>",
  "description": "Botting away at Teleport",
  "dependencies": {
    "hubot": "^2.16.0",
    "hubot-brain-inspect": "0.0.1",
    "hubot-business-cat": "^1.2.5",
    "hubot-cronjob": "^0.2.1",
    "hubot-diagnostics": "0.0.1",
    "hubot-firebase-brain": "^1.0.1",
    "hubot-fleep": "msokk/hubot-fleep",
    "hubot-help": "^0.1.1",
    "hubot-plusplus": "^1.1.6",
    "hubot-pugme": "^0.1.0",
    "hubot-reaction": "0.0.6",
    "hubot-rules": "^0.1.1",
    "hubot-seriouslyguys": "^1.0.0",
    "hubot-shipit": "^0.2.0",
    "hubot-thank-you": "0.0.3",
    "lodash": "^3.10.1",
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^0.1.0"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "default-case": 0,
      "guard-for-in": 0,
      "strict": 0,
      "id-length": [
        2,
        {
          "min": 2,
          "properties": "never",
          "exceptions": [
            "e",
            "_"
          ]
        }
      ]
    }
  }
}
